+			/* the job started after the previous doProcess and we have no 
+			 * record about it from qstat */
+			vos.run_time_t += jobRuntime; userSumm.run_time_t += jobRuntime;		
+			vos.cpu_time_t += jobCpu; userSumm.cpu_time_t += jobCpu;
