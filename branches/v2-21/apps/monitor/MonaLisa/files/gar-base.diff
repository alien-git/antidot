+							site.isFirst = false;
+							userSites.setElementAt(site,i);
+							results.add(r0);
+						}
+						voTotal = (UserFTPData) values.get(ufdPos);
+						result.addSet("ftpInput_" + voTotal.node_name, voTotal.ftpInput / factor);
+						result.addSet("ftpRateIn_" + voTotal.node_name, voTotal.ftpRateIn / factor);
