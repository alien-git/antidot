+    	  bVal = VO_Utils.getBArgumentValue(argList[j], "verboselogging", logger);
+    	  if (bVal != null) {
+    		  verboseLogging = bVal.booleanValue();   
+    		  continue;
+    	  } 
+    	  
+      } // end for 
+    } // end if args
+
+    info.ResTypes = ResTypes();
