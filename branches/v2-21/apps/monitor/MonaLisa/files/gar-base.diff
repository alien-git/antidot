+	   */
+	public static String getSArgumentValue(String listEntry, String argName,
+			Logger logger) {
+		  String retVal = null;
+		  
+		  if (!listEntry.toLowerCase().startsWith(argName))
+			  return null;
+		 
+		  try {
+			  retVal = listEntry.split("(\\s)*=(\\s)*")[1].trim();
+		  }catch(Throwable t){                      
+			  logger.log(Level.INFO, " Got exception parsing module argument " +
+					  listEntry + ": ", t);
+			  return null;
+		  }
+		  
+		 return retVal; 
+	  }
