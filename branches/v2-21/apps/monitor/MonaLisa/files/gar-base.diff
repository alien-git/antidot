+/**
+ * Updates the statistics for a user of VO with the job given as parameter
+ * @param vos VO or user statistics information
+ * @param jobInfo Information for the current job
+ * @param haveNewJob This flag is true if the job is new
+ */
+protected void updateCountStatistics(VOSummaryExt vos, JobInfoExt jobInfo,
+		boolean haveNewJob) {
+	
+	// F - finished
+	if(jobInfo.status.equals("H")) {
+		vos.heldjobs += 1;
+	} else if (jobInfo.status.equals("R")) {
+		vos.runningjobs += 1;
+	} else if (jobInfo.status.equals("I")) {
+		vos.idlejobs += 1;
+	} else if (jobInfo.status.equals("F")) {
+	    if (!(jobInfo.jobManager.equals(VO_Utils.PBS) && checkPBSHist)) {
+		// don't count the PBS finished jobs if we scan
+		// the PBS logs, because they will appear twice
+		if (haveNewJob) { // this job is not in currentJobsInfo and we have to count it
+		    vos.finishedJobs++;
+		    vos.submittedJobs++;
+		}
+		
+		if (jobInfo.exit_status ==0)
+		    vos.successJobs++;
+		else
+		    vos.errorJobs++;
+	    }
+	} else {
+	    vos.unknownjobs += 1;
+	}
+	
+	if (haveNewJob && !jobInfo.status.equals("F")) {
+		vos.submittedJobs++;
+	}
+}
