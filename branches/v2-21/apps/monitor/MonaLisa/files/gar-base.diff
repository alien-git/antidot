+                    publish(buffToPublish[i]);
+                } catch(Throwable t) {
+                    logger.log(Level.WARNING, " [ " + Name + " ] Got exc expressResults() publishing record [ " + buffToPublish[i] + " ]", t);
+                }
+            }
+        } catch ( Throwable t1){
+            logger.log(Level.WARNING, " [ " + Name + " ] Got exc expressResults() main loop", t1);
+        }
+        return null;  
+    }
