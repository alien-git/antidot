+      
+      if (isInternalModule)
+          sendStatusEmail();
+     
+      //-- update the error logging counters ----
+      refreshErrorCount();
+      
+      String infomsg =  "Execution time for doProcess() [" + execCnt + 
+			  "]: " + totalProcessingTime + " ms";
+      execCnt = (execCnt + 1) % 2000000;
