+    	  e.printStackTrace();
+      }
+      
+      /* add the results for the job manager versions */
+      // TODO change this
+      if (execCnt % 10 == 0) {
+    	  eResult jmvRes = new eResult();
+    	  jmvRes.ClusterName = Node.getClusterName() + "_Totals";
