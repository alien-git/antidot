+                        continue;
+                    }
+                    
+                    boolean policy = false;
+                    try {
+                        String sPolicy = tokens[1].trim();
+                        if (sPolicy.toLowerCase().indexOf("allow") != -1) {
+                            policy = true;
+                        } else if (sPolicy.toLowerCase().indexOf("deny") != -1) {
