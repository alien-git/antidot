+    /**
+     * @see lia.Monitor.Filters.GenericMLFilter#expressResults()
+     */
+    public Object expressResults() {   
+        try {
+            synchronized(bwLock) {
+                if(bw == null) {
+                    initWriter();
+                }
+                if(bw == null) return null;
+            }
+
+            Object[] buffToPublish = getAndClearBuffer();
+            for(int i=0; i < buffToPublish.length; i++) {
