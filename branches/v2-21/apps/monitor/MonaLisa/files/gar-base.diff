+      
+      //-- update the results counters ----
+      nCumulatedResults += v.size();
+      nCurrentResults = v.size();
+      
+      //-- save the latest results (to be included in the status email) ----
+      lastResults = new StringBuffer();
+      int maxResults = maxResultsEmailed;
